package myTest;
public class Example8_13
{
    public static void main(String args[ ])
    {
        Daemon d=new Daemon();
        d.a.start();
        d.b.setDaemon(true);
        d.b.start();
   }
}
class Daemon implements Runnable{

	Thread a,b;
	Daemon(){
		a=new Thread(this);
		b=new Thread(this);
	}
	
	@Override
	public void run() {
		// TODO 自动生成的方法存根
		
		
	}
	
}
